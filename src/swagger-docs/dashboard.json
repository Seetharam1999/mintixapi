{
    "swagger": "2.0",
    "info": {
        "title": "Wegot",
        "description": "API for WEGOT mobile app",
        "version": "1.0"
    },
    "produces": [
        "application/json"
    ],
    "host": "",
    "basePath": "/dashboard",
    "paths": {
        "/": {
            "post": {
                "tags": [
                    "Dashboard"
                ],
                "description": "List water usage for particular apartment",
                "operationId": "getUsage",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "body",
                        "in": "body",
                        "description": "Apartment details and Date range",
                        "required": "true",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "site_id": {
                                    "type": "integer"
                                },
                                "block_id": {
                                    "type": "integer"
                                },
                                "apart_id": {
                                    "type": "integer"
                                },
                                "from_date": {
                                    "type": "string"
                                },
                                "to_date": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    {
                        "name": "token",
                        "description": "API access token",
                        "in": "header",
                        "required": false,
                        "type": "string"
                    }
                ],
                "responses": {
                    "500": {
                        "description": "SERVER_ERROR"
                    },
                    "422": {
                        "description": "VALIDATION_FAILED"
                    },
                    "200": {
                        "description": "Success",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "site_id": {
                                    "type": "integer"
                                },
                                "block_id": {
                                    "type": "integer"
                                },
                                "apart_id": {
                                    "type": "integer"
                                },
                                "inlet_data": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/definitions/inletData"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "inletData": {
            "type": "object",
            "properties": {
                "inlet_name": {
                    "type": "string"
                },
                "inlet_usage": {
                    "type": "string"
                },
                "inlet_icon": {
                    "type": "string"
                },
                "last_updated_date": {
                    "type": "string"
                },
                "component_id": {
                    "type": "string"
                },
                "inlet_alarm_type": {
                    "type": "string"
                }
            }
        }
    }
}