                "usage": {
                    "type": "string"
                },
                "icon": {
                    "type": "string"
                },
                "last_updated_date": {
                    "type": "string"
                },
                "component_id": {
                    "type": "string"
                },
		"usage_KW":{
		"type":"string"
		}
            }
        }
    }
}
